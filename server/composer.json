{
    "name": "tome/tomeapi",
    "version": "1.0.0",
    "description": "The backend/server/api for T.O.M.E. - Training Organization Made Easy ",
    "keywords": ["laravel", "api", "boilerplate", "jwt", "auth", "rest"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.3.0",
        "code-lts/laravel-fcm": "^1.6.0",
        "dingo/api": "^v3.0.5",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^2.0",
        "grohiro/laravel-camelcase-json": "^2.0.0",
        "intervention/image": "^2.5",
        "laravel/framework": "^8.0",
        "laravel/tinker": "^v2.5.0",
        "maatwebsite/excel": "^3.1.26",
        "macsidigital/laravel-zoom": "^4.1",
        "spatie/laravel-permission": "^3.16.0",
        "tymon/jwt-auth": "1.0.*"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate",
            "php artisan jwt:secret -f"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
